<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>in_office</string>

  <key>ProgramArguments</key>
  <array>
    <string>ADD PATH TO IN_OFFICE BINARY HERE</string>
    <string>check</string>
  </array>

  <key>StartCalendarInterval</key>
  <array>
      <dict>
          <key>Hour</key>
          <integer>10</integer>
          <key>Minute</key>
          <integer>32</integer>
      </dict>
      <dict>
          <key>Hour</key>
          <integer>11</integer>
          <key>Minute</key>
          <integer>32</integer>
      </dict>
  </array>

  <key>RunAtLoad</key>
  <true/>

  <key>StandardErrorPath</key>
  <string>ADD PATH TO IN_OFFICE_PREFIX/stderr.txt</string>

  <key>StandardOutPath</key>
  <string>ADD PATH TO IN_OFFICE_PREFIX/stdout.txt</string>
</dict>
</plist>
